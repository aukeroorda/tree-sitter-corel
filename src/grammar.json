{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "corel",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "~"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "PATTERN",
                          "value": ".*"
                        },
                        {
                          "type": "CHOICE",
                          "members": [
                            {
                              "type": "SYMBOL",
                              "name": "exact_value"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "fraction"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "natural_number"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_RANGE"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "range"
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_EXACT_VALUE"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "exact_value"
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_NATURAL_NUMBER"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "natural_number"
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_FRACTION"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "fraction"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "~.*"
                }
              ]
            }
          ]
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "number_or_range": {
      "type": "CHOICE",
      "members": [
        {
          "type": "FIELD",
          "name": "number",
          "content": {
            "type": "SYMBOL",
            "name": "exact_value"
          }
        },
        {
          "type": "FIELD",
          "name": "range",
          "content": {
            "type": "SYMBOL",
            "name": "range"
          }
        }
      ]
    },
    "range": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "lower",
          "content": {
            "type": "SYMBOL",
            "name": "exact_value"
          }
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "FIELD",
          "name": "upper",
          "content": {
            "type": "SYMBOL",
            "name": "exact_value"
          }
        }
      ]
    },
    "exact_value": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "FIELD",
            "name": "sole_integral",
            "content": {
              "type": "SYMBOL",
              "name": "natural_number"
            }
          },
          {
            "type": "FIELD",
            "name": "mixed",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "natural_number"
                },
                {
                  "type": "SYMBOL",
                  "name": "fraction"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "sole_fraction",
            "content": {
              "type": "SYMBOL",
              "name": "fraction"
            }
          }
        ]
      }
    },
    "fraction": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "numerator",
          "content": {
            "type": "SYMBOL",
            "name": "natural_number"
          }
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "FIELD",
          "name": "denominator",
          "content": {
            "type": "SYMBOL",
            "name": "natural_number"
          }
        }
      ]
    },
    "natural_number": {
      "type": "PATTERN",
      "value": "[1-9](\\d)?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
