{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "corel",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "~"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_PRECEDENCE\n"
                        },
                        {
                          "type": "CHOICE",
                          "members": [
                            {
                              "type": "SYMBOL",
                              "name": "exact_value"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "fraction"
                            },
                            {
                              "type": "SYMBOL",
                              "name": "natural_number"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_EXACT_VALUE\n"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "exact_value"
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_NATURAL_NUMBER\n"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "natural_number"
                        }
                      ]
                    },
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "STRING",
                          "value": "TEST_FRACTION\n"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "fraction"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\n"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "~.*[\\n]*"
                }
              ]
            }
          ]
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "recipe": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "paragraph"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_inline"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_inline": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "emphasis"
          },
          {
            "type": "SYMBOL",
            "name": "_text"
          }
        ]
      }
    },
    "emphasis": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "_"
          },
          {
            "type": "SYMBOL",
            "name": "_inline"
          },
          {
            "type": "STRING",
            "value": "_"
          }
        ]
      }
    },
    "_text": {
      "type": "PATTERN",
      "value": "[^\\n]"
    },
    "rest_of_line": {
      "type": "PATTERN",
      "value": ".*[\\n]"
    },
    "exact_value": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "natural_number"
          },
          {
            "type": "SYMBOL",
            "name": "mixed"
          },
          {
            "type": "SYMBOL",
            "name": "fraction"
          }
        ]
      }
    },
    "mixed": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "natural_number"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_hwhitespace"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "fraction"
          }
        ]
      }
    },
    "fraction": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "numerator",
          "content": {
            "type": "SYMBOL",
            "name": "natural_number"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hwhitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_hwhitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "denominator",
          "content": {
            "type": "SYMBOL",
            "name": "natural_number"
          }
        }
      ]
    },
    "natural_number": {
      "type": "PATTERN",
      "value": "[1-9](\\d)?"
    },
    "_hwhitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
